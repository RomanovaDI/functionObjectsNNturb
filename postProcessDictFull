FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      postProcessDict;
}

functions
{
    readFields_U
    {
        type        readFields;
        libs        (fieldFunctionObjects);
        fields      (U);
    }

    readFields_p
    {
        type        readFields;
        libs        (fieldFunctionObjects);
        fields      (p);
    }

    readFields_R
    {
        type        readFields;
        libs        (fieldFunctionObjects);
        fields      (R);
    }

    writeCellCentres
    {
        type        writeCellCentres;
        libs        (fieldFunctionObjects);
    }

    writeCellVolumes
    {
        type            writeCellVolumes;
        libs            (fieldFunctionObjects);
    }

    Lambda2
    {
        type            Lambda2;
        libs            (fieldFunctionObjects);
        executeControl  writeTime;
        writeControl    writeTime;
    }

    gradLambda2
    {
        type            grad;
        libs            (fieldFunctionObjects);
        field           Lambda2;
        executeControl  writeTime;
        writeControl    writeTime;
    }

    magGradLambda2
    {
        type            mag;
        libs            (fieldFunctionObjects);
        field           grad(Lambda2);
        executeControl  writeTime;
        writeControl    writeTime;
    }

    Q
    {
        type            Q;
        libs            (fieldFunctionObjects);
        executeControl  writeTime;
        writeControl    writeTime;
    }

    gradQ
    {
        type            grad;
        libs            (fieldFunctionObjects);
        field           Q;
        executeControl  writeTime;
        writeControl    writeTime;
    }

    magGradQ
    {
        type            mag;
        libs            (fieldFunctionObjects);
        field           grad(Q);RT/RT.C

        executeControl  writeTime;
        writeControl    writeTime;
    }

    LambVector
    {
        type            LambVector;
        libs            (fieldFunctionObjects);
        executeControl  writeTime;
        writeControl    writeTime;
    }

    magLambVector
    {
        type            mag;
        libs            (fieldFunctionObjects);
        field           LambVector;
        executeControl  writeTime;
        writeControl    writeTime;
    }

    gradMagLambVector
    {
        type            grad;
        libs            (fieldFunctionObjects);
        field           mag(LambVector);
        executeControl  writeTime;
        writeControl    writeTime;
    }

    magGradMagLambVector
    {
        type            mag;
        libs            (fieldFunctionObjects);
        field           grad(mag(LambVector));
        executeControl  writeTime;
        writeControl    writeTime;
    }

